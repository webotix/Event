<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Srirastu Wedding Event And Planner</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <!-- Stylesheets -->
    <link href='@Url.Content("~/Content/css/bootstrap.min.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/css/flipclock.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/css/style.css")' rel="stylesheet" type="text/css" />
    <!-- Responsive -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link href='@Url.Content("~/Content/css/bootstrap-margin-padding.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/css/responsive.css")' rel="stylesheet" type="text/css" />
    <script src='@Url.Content("~/Content/js/jquery.js")'></script>
    <script src='@Url.Content("~/Content/js/bootstrap.min.js")'></script>
    <script src='@Url.Content("~/Content/js/js-collection.js")'></script>
    <script src='@Url.Content("~/Content/js/flipclock.js")'></script>
    <script src='@Url.Content("~/Content/js/jquery-ui.min.js")'></script>
    <script src='@Url.Content("~/Content/js/script.js")'></script>

</head>
<body>
    <div class="page-wrapper">
        <div>
            @Html.Partial("_HeaderPartial")
        </div>
        <div>
            @RenderBody()
            <hr />
            <footer>
                <div>
                    @Html.Partial("_FooterPartial")
                </div>

            </footer>
        </div>
        </div>
        <script src="~/Content/Scripts/jquery-1.10.2.min.js"></script>
        <script src="~/Conent/Scripts/bootstrap.min.js"></script>
</body>
</html>